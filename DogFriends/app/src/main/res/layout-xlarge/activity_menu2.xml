<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controlador.Menu">

    <ImageView
        android:id="@+id/btnveterinarias"
        android:layout_width="843dp"
        android:layout_height="297dp"
        android:src="@drawable/btnveterinaria"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="239dp"
        android:layout_height="93dp"
        android:textSize="50sp"
        android:fontFamily="@font/letra"
        android:text="veterinarias"
        android:textColor="#672117"

        app:layout_constraintBottom_toBottomOf="@+id/btnveterinarias"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnveterinarias" />


    <ImageView
        android:id="@+id/btnnotas"
        android:layout_width="800dp"
        android:layout_height="310dp"
        android:layout_marginTop="8dp"

        android:src="@drawable/btnnotas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnveterinarias"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:layout_width="141dp"
        android:layout_height="93dp"
        android:fontFamily="@font/letra"
        android:text="Notas"
        android:textColor="#672117"
        android:textSize="50sp"

        app:layout_constraintBottom_toBottomOf="@+id/btnnotas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/btnnotas"
        app:layout_constraintTop_toTopOf="@+id/btnnotas" />


    <ImageView
        android:id="@+id/btnhistorias"
        android:layout_width="829dp"
        android:layout_height="335dp"
        android:src="@drawable/btnhistorias"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnnotas" />


    <TextView
        android:layout_width="336dp"
        android:layout_height="84dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/letra"
        android:text="Hisotiras Clinicas"
        android:textColor="#672117"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="@+id/btnhistorias"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnnotas" />

    <ImageView
        android:id="@+id/btncinfigcount"
        android:layout_width="154dp"
        android:layout_height="72dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/configuracuenta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.958"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/btnconfigapp"
        android:layout_width="154dp"
        android:layout_height="72dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/configapp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>